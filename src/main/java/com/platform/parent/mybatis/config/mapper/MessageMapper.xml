<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.platform.parent.mybatis.dao.MessageMapper">

    <insert id="add" parameterType="com.platform.parent.mybatis.bean.Message">
        INSERT INTO `message` (`msg_id`, `direction`, `to`, `from`,`chat_type`)
                VALUES (#{msg_id}, #{direction}, #{to}, #{from}, #{chatType})
    </insert>
</mapper>